{
  "name": "@workflow/world-local",
  "version": "4.0.1-beta.26",
  "description": "Local development World implementation for Workflow DevKit",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/workflow.git",
    "directory": "packages/world-local"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "genversion --es6 src/version.ts && tsc",
    "dev": "genversion --es6 src/version.ts && tsc --watch",
    "clean": "tsc --build --clean && rm -rf dist src/version.ts",
    "test": "vitest run src",
    "typecheck": "genversion --es6 src/version.ts && tsc --noEmit"
  },
  "dependencies": {
    "@vercel/queue": "catalog:",
    "@workflow/errors": "workspace:*",
    "@workflow/utils": "workspace:*",
    "@workflow/world": "workspace:*",
    "async-sema": "3.1.1",
    "ulid": "3.0.1",
    "undici": "6.22.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.9.0",
    "@types/ms": "0.7.34",
    "@types/node": "catalog:",
    "@workflow/tsconfig": "workspace:*",
    "genversion": "3.2.0",
    "ms": "2.1.3",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@opentelemetry/api": "1"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  }
}
